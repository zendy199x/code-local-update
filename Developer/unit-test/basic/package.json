{
  "name": "unit-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "tslint -c ./tslint.json --project ./tsconfig.json \"src/**/*.{ts,tsx}\" -e \"**/eds/**\"",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose --i",
    "test:coverage": "jest --verbose --coverage",
    "test:update_snapshots": "jest --verbose -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arp82/samples-for-react-jest-enzyme-ts-unit-test-training.git"
  },
  "author": "zendy",
  "bugs": {
    "url": "https://github.com/arp82/samples-for-react-jest-enzyme-ts-unit-test-training/issues"
  },
  "homepage": "https://github.com/arp82/samples-for-react-jest-enzyme-ts-unit-test-training#readme",
  "devDependencies": {
    "@types/deep-freeze": "0.1.1",
    "@types/enzyme": "3.1.10",
    "@types/jest": "22.2.3",
    "@types/lodash-es": "4.17.0",
    "@types/react": "16.4.1",
    "@types/react-dom": "16.0.6",
    "@types/react-redux": "6.0.2",
    "@types/react-router": "3.0.15",
    "@types/redux-mock-store": "0.0.13",
    "awesome-typescript-loader": "3.5.0",
    "axios-mock-adapter": "1.15.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "deep-freeze": "0.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.4",
    "node-sass": "4.9.0",
    "redux-mock-store": "1.4.0",
    "sass-loader": "6.0.7",
    "speed-measure-webpack-plugin": "1.2.2",
    "style-loader": "0.19.1",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2",
    "webpack": "3.12.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-server": "2.11.2",
    "webpack-merge": "4.1.3"
  },
  "dependencies": {
    "@material-ui/core": "1.4.3",
    "@material-ui/icons": "1.1.0",
    "@turf/circle": "6.0.1",
    "axios": "0.17.1",
    "babel-polyfill": "6.26.0",
    "lodash-es": "4.17.10",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router": "3.2.1",
    "redux": "3.7.2",
    "redux-thunk": "2.3.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "testRegex": "\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "setupFiles": [
      "<rootDir>/config/test/polyfills.js",
      "<rootDir>/config/test/setupTest.js"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/.*)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.s?(css|svg)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/resources/**"
    ]
  }
}
